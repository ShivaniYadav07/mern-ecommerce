{
    "version": 2,
    "name": "mern-ecommerce",
    "builds": [
      { "src": "server/app.js", "use": "@vercel/node" },
      { "src": "client/build/", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "server/app.js" },
      { "src": "/(.*)", "dest": "client/build/$1" }
    ]
  }